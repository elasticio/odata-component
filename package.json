{
  "name": "odata-component",
  "version": "3.0.0",
  "description": "A component designed to work with generic OData services.",
  "homepage": "https://elastic.io",
  "author": {
    "name": "Elastic.io GmbH",
    "email": "ps@elastic.io",
    "url": "https://elastic.io"
  },
  "main": "module.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "pretest": "eslint verifyCredentials.js lib spec spec-integration --fix",
    "test": "mocha spec/**",
    "integration-test": "mocha spec-integration"
  },
  "engines": {
    "node": "8"
  },
  "keywords": [
    "OData",
    "elasticio-component",
    "Connector"
  ],
  "dependencies": {
    "@elastic.io/odata-library": "0.0.1",
    "odata2openapi": "1.3.0",
    "elasticio-node": "0.0.9",
    "elasticio-sailor-nodejs": "2.2.1",
    "lodash": "4.17.10",
    "remove-leading-slash": "1.0.1",
    "remove-trailing-slash": "0.1.0",
    "request": "2.85.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "dotenv": "4.0.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.18.0",
    "mocha": "4.0.1",
    "nock": "9.6.1",
    "nsp": "3.2.1",
    "sinon": "5.1.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "plugins": [
      "node"
    ],
    "env": {
      "mocha": true,
      "node": true
    },
    "rules": {
      "no-console": "off"
    }
  },
  "repository": "https://github.com/elasticio/odata-component/",
  "license": "Apache-2.0"
}
